<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <servlet>
        <servlet-name>SubmissionHandler</servlet-name>
        <servlet-class>citeunseen.SubmissionHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmissionHandler</servlet-name>
        <url-pattern>/results</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClickHandler</servlet-name>
        <servlet-class>citeunseen.ClickHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClickHandler</servlet-name>
        <url-pattern>/overlap</url-pattern>
    </servlet-mapping>
  
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>